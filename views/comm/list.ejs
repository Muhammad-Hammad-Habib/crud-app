<table class="table table-striped-columns">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    <% var i = 1; %>
    <% data.forEach(item => { %>
    <tr>
      <th scope="row"><%= i++ %></th>
      <td><%= item.name %></td>
      <td><%= item.age %></td>
      <td><%= item.date %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
